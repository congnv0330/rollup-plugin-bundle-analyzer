{
  "name": "rollup-plugin-bundle-analyzer",
  "version": "1.6.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "rollup plugin that represents bundle content as convenient interactive zoomable treemap",
  "license": "MIT",
  "author": "xiaojie",
  "homepage": "",
  "bugs": "",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    "import": "./dist/es/index.js",
    "types": "./types/index.d.ts",
    "default": "./dist/cjs/index.js"
  },
  "types": "./types/index.d.ts",
  "ava": {
    "files": [
      "!**/fixtures/**",
      "!**/output/**",
      "!**/helpers/**",
      "!**/recipes/**",
      "!**/types.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": "https://github.com/Jerry2023/rollup-plugin-bundle-analyzer",
  "scripts": {
    "build:client": "vite build",
    "build-all": "pnpm run build:client & pnpm run build",
    "build": "rollup -c",
    "ci:coverage": "nyc pnpm test && nyc report --reporter=text-lcov > coverage.lcov",
    "ci:lint": "pnpm build && pnpm lint",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "ci:test": "pnpm test -- --verbose",
    "prebuild": "del-cli dist",
    "prepare": "if [ ! -d 'dist' ]; then pnpm build; fi",
    "prerelease": "pnpm build",
    "pretest": "pnpm build",
    "release": "pnpm --workspace-root plugin:release --pkg $npm_package_name",
    "test": "ava",
    "test:ts": "tsc --noEmit"
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "types",
    "README.md",
    "public",
    ""
  ],
  "keywords": [
    "vite",
    "rollup",
    "plugin",
    "resolve",
    "bundle",
    "size"
  ],
  "peerDependencies": {
    "rollup": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "gzip-size": "^7.0.0",
    "lodash": "^4.17.21",
    "opener": "^1.5.2",
    "sirv": "^2.0.3",
    "terser": "^5.24.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.23.3",
    "@carrotsearch/foamtree": "^3.5.1",
    "@preact/preset-vite": "^2.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash": "^4.14.202",
    "@types/opener": "^1.4.3",
    "@types/uglify-js": "^3.17.4",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.16",
    "chai": "^4.3.10",
    "chai-subset": "^1.6.0",
    "classnames": "^2.3.2",
    "core-js": "^3.33.3",
    "css-loader": "^6.2.7",
    "cssnano": "^6.0.1",
    "del": "^7.1.0",
    "del-cli": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "filesize": "^10.0.0",
    "globby": "^14.0.0",
    "jest": "^29.0.0",
    "mobx": "^6.11.0",
    "mobx-react": "^9.1.0",
    "postcss": "^8.4.31",
    "postcss-icss-values": "^2.0.2",
    "postcss-loader": "^7.3.3",
    "preact": "^10.19.2",
    "rollup": "^4.5.1",
    "stream-combiner2": "^1.1.1",
    "style-loader": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "5.3.2",
    "url-loader": "^4.1.1",
    "vite": "^5.0.2"
  }
}
